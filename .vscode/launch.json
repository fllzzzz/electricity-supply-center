{
	"version": "0.2.0",
	"configurations": [
	  {
		"type": "chrome",
		"request": "launch",
		"name": "vue-chrome",
		"url": "http://localhost:8080/#/demo",
		"webRoot": "${workspaceFolder}",
		"skipFiles": [
			"<node_internals>/**",
			"${webRoot}/node_modules/**",
			"${webRoot}/webpack/**",
			"**/HeaderClock.vue**",
		]
	  }
	]
  }